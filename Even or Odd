package Fundamentals;
import java.util.Scanner;
public class EvenOdd {
	public static String evenNo(int a){
		if(a%2==0) {
			return "Even";
		}
		else {
			return "Odd";
		}
	}
	public static void main(String[] args) {
		Scanner Sc = new Scanner(System.in);
		int n = Sc.nextInt();
		System.out.println(evenNo(n));
	}
}
