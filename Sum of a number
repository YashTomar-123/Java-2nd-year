package Fundamentals;
import java.util.Scanner;
public class Sumdigits {
	public static int issum(int a) {
		int sum = 0;
		int i = 0;
		while(a>0) {
			sum = sum + (a%10);
			a = a/10;
		}
		return sum;
	}
	public static void main(String[] args) {
		Scanner Sc = new Scanner(System.in);
		int n = Sc.nextInt();
		System.out.println(issum(n));
	}
}
