package Fundamentals;
import java.util.Scanner;
public class Patternabc {
	public static void main(String[] args) {	
		Scanner Sc = new Scanner(System.in);
		int rows = Sc.nextInt();
		char i = 1;
		while(i<=(96+rows)) {
			char a = 97;
			while(a <= i) {
				System.out.print(a);
				a++;
			}
			//preparation for next row
			System.out.println();
			i++;
		}
	}
}
