package Fundamentals;
import java.util.Scanner;
public class Pattern2 {
	public static void main(String[] args) {
		Scanner Sc = new Scanner(System.in);
		int n = Sc.nextInt();  
		int row = 1;
		while(row<=n) {
			int Column = 1;
			while(Column<=row) {
				System.out.print(Column);
				Column++;
			}
			//preparation for next line
			System.out.println();
			row++;
		}
	}

}
