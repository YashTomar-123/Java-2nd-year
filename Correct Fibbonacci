import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner Sc = new Scanner(System.in);
		System.out.println("Enter your no. of terms");
		int n = Sc.nextInt();
		int a = 0,b = 1;
		int c;
		System.out.println(a);
		System.out.println(b);
		for(int i = 1;i<(n-1);i++){
		    c = a + b;
		    a = b;
		    b = c;
		    System.out.println(b);
		}
}
}
